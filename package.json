{
  "name": "frontend-mentor-rock-paper-scissor-lizard-spock",
  "version": "0.1.0",
  "description": "A simple starter project to get up and developing quickly with the blend of Svelte and Electron JS for building a Native Desktop App on various OS Platforms like Windows, Linux and macOS using Electron Builder.",
  "main": "main.js",
  "author": "Aalekh Patel <itsme@aalekhpatel.com>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/aalekhpatel07/frontend-mentor-rock-paper-scissors-lizard-spock"
  },
  "bugs": {
    "url": "https://github.com/aalekhpatel07/frontend-mentor-rock-paper-scissors-lizard-spock/issues"
  },
  "build": {
    "icon": "public/favicon.png",
    "productName": "Rock Paper Scissors Lizard Spock",
    "files": [
      "public/**/*",
      "main.js"
    ],
    "win": {},
    "linux": {},
    "mac": {}
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "electron": "wait-on http://localhost:5000 && electron .",
    "electron-dev": "concurrently \"npm run dev\" \"npm run electron\"",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder"
  },
  "dependencies": {
    "electron-serve": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "concurrently": "^5.3.0",
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "~6.1.1",
    "rollup-plugin-terser": "^6.0.0",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.0.0",
    "wait-on": "^5.2.0"
  },
  "keywords": [
    "cross-platform",
    "native-app",
    "installed-app",
    "desktop-applications",
    "windows",
    "linux",
    "macos",
    "html",
    "css",
    "javascript",
    "front-end",
    "frontend-mentor",
    "rock",
    "paper",
    "scissors",
    "lizard",
    "spock",
    "game",
    "compiler",
    "svelte",
    "rollup",
    "electron",
    "electron-builder",
    "electron-serve",
    "concurrently",
    "wait-on",
    "webapp"
  ]
}
